<div class="row">
    <div class="col-sm-3 profile">
        <div class="profile-header">
            <div class="profile-image">
                <img class="sr-only" src="http://placehold.it/100x100">
            </div>
        </div>
    </div>
</div>
<div class="projects" id="projects">
    {{#each projects}}
    <div class="row">
        <div class="col-sm-8 col-sm-offset-3 project">
            <h1 class="title">{{this.title}}</h1>
            <h4>{{this.company}}</h4>
            <p>{{this.start}}{{#if this.end}} - {{this.end}}{{/if}}</p>
            <p>{{this.intro}}</p>
            <a href="/projects/{{this.slug}}.html" class="btn btn-primary">Read more</a>
        </div>
    </div>
    {{/each}}
</div>